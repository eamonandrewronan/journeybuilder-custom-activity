{
  "workflowApiVersion": "1.1",
  "metaData": {
    "icon": "images/contract_doc_120.png"
  },
  "type": "REST",
  "lang": {
    "en-US": {
      "description": "Custom Activity"
    }
  },
  "arguments": {
    "execute": {
      "inArguments": [
        {
          "contactIdentifier": "{{Contact.Key}}"
        },
        {
          "Email": "{{Event.TEST_EntryJourney.Email}}"
        },
        {
          "OpportunityID": "{{Event.TEST_EntryJourney.OpportunityID}}"
        },
        {
          "CampaignID": "{{Event.TEST_EntryJourney.CampaignID}}"
        },
        %%TYPECOMMSCONFIG%%,
        %%TYPECONFIG%%,
        %%IMAGECONFIG%%
      ],
      "outArguments": [
        {
          "foundSignupDate": ""
        }
      ],
      "url": "https://$DOMAIN/journey/execute",
      "verb": "POST",
      "body": "",
      "header": "",
      "format": "json",
      "useJwt": true,
      "timeout": 10000
    }
  },
  "configurationArguments": {
    "save": {
      "url": "https://$DOMAIN/journey/save",
      "verb": "POST",
      "useJwt": true
    },
    "publish": {
      "url": "https://$DOMAIN/journey/publish",
      "verb": "POST",
      "useJwt": true
    },
    "stop": {
      "url": "https://$DOMAIN/journey/stop",
      "verb": "POST",
      "useJwt": true
    },
    "validate": {
      "url": "https://$DOMAIN/journey/validate",
      "verb": "POST",
      "useJwt": true
    }
  },
  "userInterfaces": {
    "configInspector": {
      "hideHeader": true,
      "size": "scm-sm"
    }
  }
}
